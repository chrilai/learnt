			android:screenOrientation="portrait"
            android:label="@string/labyrinth" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="lai.christine.hw5.InstructionActivity"
            android:label="@string/labyrinth" >
        </activity>
    </application>

</manifest>
